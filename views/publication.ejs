    <!-- Demo content -->


    <h3 style="text-align: center;color: purple;">Congrats on new publication&nbsp;&nbsp;&nbsp;&nbsp;<i
        class="fa fa-gift" style="color: blue;"></i></h3>
    <br/>
    <h5 style="text-align: center;color: darkgreen;"><i>Please fill the details here:</i></h5>
    <br/>

    <form class="publication" action="/publication" method="post">
      <div class="form-group row">
        <label for="Title" class="col-md-2 col-form-label required"><strong>Title of the paper</strong></label>
        <div class="col-md-10">
          <input type="text" class="form-control" name="titleOfThePaper" id="Title" placeholder="Title of the paper">
        </div>
      </div>
      <div class="form-group row">
        <label for="PublicationType" class="col-md-2 col-form-label required"><strong><i>Publication Type</i></strong></label>
        <div class="col-md-10">
          <select class = "form-control" name="publicationType" id="PublicationType">
            <option value="journal">Journal</option>
            <option value="conference">Conferece</option>
            <option value="book">Book</option>
            <option value="patent">Patent</option>
            <option value="book chapter name">Book chapter Name</option>
          </select>
        </div>
      </div>
      <div class="form-group row">
        <label for="publicationCategory" class="col-md-2 col-form-label required"><strong><i>Publication Category</i></strong></label>
        <div class="col-md-10">
          <select class = "form-control" name="publicationCategory" id="PublicationCategory">
            <option value = "national">National</option>
            <option value = "international">International</option>
          </select>
        </div>
      </div>
      <div class="form-group row">
        <label for="Authors" class="col-md-2 col-form-label required"><strong><i>List of authors</i></strong></label>
        <div class="col-md-10">
          <textarea type="text" class="form-control" name="listOfAuthors" id="Authors">1</textarea>
        </div>

      </div>
      <div class="form-group row">
        <label for="Conferece/Journal" class="col-md-2 col-form-label required"><strong><i>Conference/Journal in which Paper was published</i></strong></label>
        <div class="col-md-10">
          <input type="text" class="form-control" name="conferenceJournal" id="Conferece/Journal">
        </div>
      </div>
      <div class="form-group row">
        <label for="Date" class="col-md-2 col-form-label required"><strong><i>Date of Publication</i></strong></label>
        <div class="col-md-10">
          <input type="date" class="form-control" name="dateOfPublication" id="Date" placeholder="date">
        </div>
      </div>


      <div class="form-row">
        <div class="form-group row col-md-7">
          <label for="PageNos" class = "col-form-label col-md-4 required"><strong><i>volume issue page numbers</i></strong></label>
          <input type="text" class="form-control col-md-8" name="volumePageNos" id="PageNos">
        </div>
        <div class="form-group row col-md-1">
        </div>
        <div class="form-group row col-md-4">
          <label for="ISBN/ISSN" class = "col-form-label col-md-4 required"><strong><i>ISBN/ISSN number</i></strong></label>
          <input class="form-control col-md-8" type="text" name="isbn/issn" id="ISBNorISSN" pattern="[0-9]{4}-[0-9]{4}">
        </div>
      </div>

      <div class="form-row">
        <div class="form-group row col-md-7">
          <label for="ImpactFactor" class = "col-form-label col-md-4 required"><strong><i>Impact Factor</i></strong></label>
          <input type="text" class="form-control col-md-8" name="impactFactor" id="ImpactFactor">
        </div>
        <div class = "form-group row col-md-1">

        </div>
        <div class="form-group row col-md-4">
          <label for="Indexed" class = "col-form-label col-md-4 required"><strong><i>Indexed in</i></strong></label>
          <select class = "form-control col-md-8" name="indexedIn" id="Indexed">
            <option value="SCOPUS">SCOPUS</option>
            <option value="Google Scholar">Google Scholar</option>
            <option value="Web Of Science">Web Of Science</option>
          </select>
        </div>
      </div>

      <div class="form-group row">
        <label for="DocumentName" class="col-md-2 col-form-label required"><strong><i>Supporting document for indexing</i></strong></label>
        <div class="col-md-10">
          <div class="form-group">

    <input type="file" class="form-control-file" id="exampleFormControlFile1">
  </div>
        </div>
      </div>

      <div class="form-group row">
        <label for="Institute" class="col-md-2 col-form-label required"><strong><i>Institute associated at time of publication</i></strong></label>
        <div class="col-md-10">
          <input type="text" class="form-control" name="institute" id="Institute" placeholder="Enter Institute">
        </div>
      </div>

      <div class="form-group row">
        <label for="NoOfCitiations" class="col-md-2 col-form-label required"><strong><i>Enter No of Citiations</i></strong></label>
        <div class="col-md-10">
          <input type="text" class="form-control" name="noOfCitiations" id="NoOfCitiations" placeholder="Enter No of Citiations">
        </div>
      </div>

      <fieldset class="form-group">
        <div class="row">
          <legend class="col-form-label col-md-2 pt-0"><strong><i>UGC listed?</i></strong></legend>
          <div class="col-md-10">
            <div class="form-check form-check-inline">
              <input class="form-check-input" type="radio" name="ugcListed" id="UgcListed" value="Yes" checked>
              <label class="form-check-label" for="UgcListed" style="font-size: 20px;">
                Yes
              </label>
            </div>
            <div class="form-check form-check-inline">
              <input class="form-check-input" type="radio" name="ugcListed" id="UgcListed" value="No">
              <label class="form-check-label" for="UgcListed "style="font-size: 20px;">
                No
              </label>
            </div>
          </div>
        </div>
      </fieldset>

      <div class="form-group row">
        <label for="DocumentForUGCListing" class="col-md-2 col-form-label required"><strong><i>Supporting document for UGC listing</i></strong></label>
        <div class="col-md-10">
          <div class="form-group">

    <input type="file" class="form-control-file" id="exampleFormControlFile1">
  </div>
        </div>
      </div>
      <div class="form-group row">
        <div class="col-md-12">
          <div class="form-check">
            <input class="form-check-input" type="checkbox" id="gridCheck1" checked>
            <label class="form-check-label" for="gridCheck1">
              <strong><i>Are You Sure to Submit</i></strong>
            </label>
          </div>
        </div>
      </div>
      <div class="form-group row">
        <div class="col-md-10 offset-md-6">
          <button type="submit" class="btn btn-primary">Submit</button>
        </div>
      </div>
    </form>




